{
    "buildCommand": "python manage.py migrate",
    "builds": [
        {
            "src": "Forms/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Forms/wsgi.py"
        }
    ],
    "public": "static" 
